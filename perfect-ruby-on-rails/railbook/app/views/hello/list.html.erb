<table border = "1">
    <tr>
        <th>ISBN 코드</th>
        <th>이름</th>
        <th>가격</th>
        <th>출판사</th>
        <th>츨판일</th>
        <th>CD-ROM</th>
    </tr>
    <% @books.each do |book| %>
        <tr>
            <td><%= book.isbn %></td>
            <td><%= book.title %></td>
            <td><%= book.price %></td>
            <td><%= book.publish %></td>
            <td><%= book.published %></td>
            <td><%= book.cd %></td>
            </td>
        </tr>
    <% end %>
</table>